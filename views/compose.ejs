<%- include('partials/header'); -%>
<h1>
  compose
</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" id="postTitle" name="postTitle">
  </div>
  <div class="form-group">
   <label for="postContent">Post</label>
   <textarea class="form-control" id="postContent" name="postContent" rows="8"></textarea>
 </div>
  <button class="btn btn-primary"  style=" font-size:24px; background-color: #1abc9c;" type="submit" name="button">Publish</button>
</form>

<%- include('partials/footer'); -%>
